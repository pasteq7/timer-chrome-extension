{
    "manifest_version": 3,
    "name": "Timer Extension",
    "version": "1.0",
    "description": "A simple timer extension",
    "permissions": ["alarms", "storage", "notifications"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }